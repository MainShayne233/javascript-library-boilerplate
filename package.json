{
  "name": "javascript-library-boilerplate",
  "version": "0.0.1",
  "description": "Boilerplate for starting a JavaScript library",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p",
    "test": "mocha --require babel-register",
    "test:watch": "npm run test -- --watch",
    "flow": "flow",
    "lint": "eslint {src,test}/**/*.js webpack.config.js",
    "format": "prettier {src,test}/**/*.js  webpack.config.js  --write",
    "precommit:install": "./node_modules/precommit/bin/cli.js install -c .precommit.json",
    "precommit:uninstall": "./node_modules/precommit/bin/cli.js remove"
  },
  "author": "MainShayne233",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "flow-bin": "^0.68.0",
    "mocha": "^5.0.5",
    "precommit": "^1.2.2",
    "prettier": "^1.11.1",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13"
  }
}
